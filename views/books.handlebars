{{> search}}
<ul class="catalog">
  {{#each books}}
  {{> book}}
  {{/each}}
</ul>
<script type="module">
  import { Api } from '/api.js'
  Api.fetch('books')
    .then((books) => {
      const pre = document.createElement('pre')
      pre.textContent = JSON.stringify(books, null, 2)
      document.body.append(pre)
    })
</script>